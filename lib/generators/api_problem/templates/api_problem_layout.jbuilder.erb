<% if options.type == "true" %>
json.set! :type, <%= error_url %>
<% elsif options.type != "false" %>
json.set! :type, <%= options.type %>"
<% end %>
<% if options.title %>json.set! :title, "<%= options.title %>"<% end %>
<% if options.status %>json.set! :status, <%= options.status %><% end %>
<% if options.detail %>json.set! :detail, "<%= options.detail %>"<% end %>
<% if options.instance %>json.set! :instance, "<%= options.instance %>"<% end %>
